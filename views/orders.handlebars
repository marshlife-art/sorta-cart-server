{{#each orders}}
<div style="margin-bottom: 50px;">
  <table>
    <thead>
      <tr>
        <th>id</th>
        <th>status</th>
        <th>payment_status</th>
        <th>shipment_status</th>
        <th>name</th>
        <th>email</th>
        <th>phone</th>
        <th>address</th>
        <th>notes</th>
        <th>createdAt</th>
        <th>updatedAt</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>{{this.id}}</td>
        <td>{{this.status}}</td>
        <td>{{this.payment_status}}</td>
        <td>{{this.shipment_status}}</td>
        <td>{{this.name}}</td>
        <td>{{this.email}}</td>
        <td>{{this.phone}}</td>
        <td>{{this.address}}</td>
        <td>{{this.notes}}</td>
        <td>{{this.createdAt}}</td>
        <td>{{this.updatedAt}}</td>

      </tr>
    </tbody>
  </table>
  <table>
    <thead>
      <tr>
        <th>vendor</th>
        <th>description</th>
        <th>qty</th>
        <th>unit</th>
        <th align="right">price</th align="right">
        <th align="right">total</th>
      </tr>
    </thead>
    <tbody>
      {{#each this.OrderLineItems}}
      <tr>
        <td>{{this.vendor}}</td>
        <td>{{this.description}}</td>
        <td>{{this.quantity}}</td>
        <td>{{this.selected_unit}}</td>
        <td align="right">{{this.price}}</td>
        <td align="right">{{this.total}}</td>
      </tr>
      {{/each}}
      <tr>
        <td colspan="4"></td>
        <td colspan="2" align="right">TOTAL</td>
      </tr>
      <tr>
        <td colspan="5"></td>
        <td align="right">{{this.total}}</td>
      </tr>
    </tbody>
  </table>
</div>
{{/each}}