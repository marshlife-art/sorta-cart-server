{
  "name": "sorta-cart-server",
  "version": "0.0.4",
  "description": "API for sorta-cart",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "NODE_ENV=test nyc --reporter=text mocha --recursive",
    "test:coverage": "NODE_ENV=test nyc --reporter=text --reporter=html mocha --recursive && nyc report --reporter=text > coverage.txt",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed:all": "sequelize-cli db:seed:all",
    "create:admin": "node util/cliCreateAdmin.js",
    "build": "react-scripts build"
  },
  "repository": "github:marshlife-art/sorta-cart-server",
  "author": "edwardsharp",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "json2csv": "^4.5.4",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "square-connect": "^2.20191217.0"
  },
  "devDependencies": {
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "sqlite3": "^4.1.0"
  }
}
